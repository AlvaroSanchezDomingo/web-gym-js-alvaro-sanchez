<h2 class="ui header">
   Goals
</h2>
<table class="ui table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Measurement</th>
      <th>Status</th>
      <th>Missed</th>
      <th>Achieved</th>
    </tr>
  </thead>
  <tbody>
    {{#each member.goals}}
      <tr>
        <td>
          {{date}}
        </td>
        <td>
          {{measurement}}
        </td>
        <td>
          {{status}}
        </td>
        <td>
          <a href="/member/{{../member.id}}/missed/{{id}}" class="ui icon button">
           <i class="thumbs down icon"></i>
          </a>
        </td>
        <td>
          <a href="/member/{{../member.id}}/achieve/{{id}}" class="ui icon button">
           <i class="thumbs up icon"></i>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>